{
  "name": "holmes-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "dev:local": "ENVIRONMENT=local-backend react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "unused": "knip",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "verify": "concurrently -n test,lint,prettier,types,unused -c green,yellow,magenta,cyan,blue \"npm run test\" \"npm run lint\" \"npm run prettier\" \"npm run typecheck\" \"npm run unused\""
  },
  "dependencies": {
    "@grafana/faro-react": "^2.1.0",
    "@navikt/aksel-icons": "^8.2.0",
    "@navikt/ds-css": "^8.2.0",
    "@navikt/ds-react": "^8.2.0",
    "@navikt/ds-tailwind": "^8.2.0",
    "@navikt/oasis": "^4.1.0",
    "@react-router/node": "^7.9.4",
    "@react-router/serve": "^7.9.4",
    "@tailwindcss/postcss": "^4.1.13",
    "autoprefixer": "^10.4.21",
    "isbot": "^5",
    "mixpanel-browser": "^2.71.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.0.0",
    "react-hotkeys-hook": "^5.2.0",
    "react-router": "^7.9.4",
    "tailwindcss": "^4.1.13",
    "unleash-client": "^6.7.0",
    "zod": "^4.1.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.56.1",
    "@react-router/dev": "^7.9.4",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^25.0.7",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^28.0.0",
    "knip": "^5.66.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "react-router-devtools": "^6.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.18"
  }
}
